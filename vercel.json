{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.7.3" }
    },
    "routes": [
        { "src": "/js/(.*)", "dest": "/public/s/$1" },
        { "src": "/css/(.*)", "dest": "/public/css/$1" },
        { "src": "/images/(.*)", "dest": "/public/images/$1" },
        { "src": "/favicon.ico", "dest": "/public/favicon.ico" },
        { "src": "/robots.txt", "dest": "/public/robots.txt" },
        { "src": "/(.*)", "dest": "/api/index.php" }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",

        "APP_URL": "https://najmat-al-thuraya.vercel.app",
        "ASSET_URL": "https://najmat-al-thuraya.vercel.app",
        "APP_KEY": "base64:/s4afqEpcPnoWCseCj6HGHzfJBemaw0p8uaqNKTZnrA=",

        "DB_CONNECTION": "sqlite",

        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",

        "SESSION_DRIVER": "cookie",
        "QUEUE_CONNECTION": "sync",
        "CACHE_STORE": "array",
        "LOG_CHANNEL": "stderr"
    }
}
